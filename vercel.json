{

    "builds": [
        {

        "src": "CDMP/wsgi.py",

        "use": "@vercel/python",

        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }

    },{
        "src":"build_file.sh"
        
    }],

    "routes": [
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },

        {

            "src": "/(.*)",

            "dest": "CDMP/wsgi.py"

        }

    ]

}